@model IEnumerable<SoloDashboard.Entities.Models.Job>

@{
    Layout = "~/Views/Home/_myLayout.cshtml";
    
}
<title>@ViewBag.Title</title>
<div>
    <div class="row">
        <div class="col-md-6">
            <h2><span class="badge badge-pill badge-info">Total Records Returned: @Model.Count()</span></h2>
        </div>
        <div class="col-md-6" style="text-align:right;">
            <h2><span class="badge badge-pill badge-info">@ViewBag.Title</span></h2>
        </div>
    </div>
    
    @foreach (var item in Model)
    {
        @Html.Partial("_ScheduleJob", item)
    }
</div>


<!-- jQuery CDN - Slim version (=without AJAX) -->
<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
